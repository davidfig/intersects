{
  "name": "intersects",
  "version": "2.7.1",
  "description": "a simple collection of 2d collision/intersects functions, supporting points, circles, circle outlines (circumference), lines, axis-aligned boxes, and polygons (convex)",
  "main": "src/_index.js",
  "common": "dist/intersects.cjs.min.js",
  "module": "dist/intersects.esm.min.js",
  "scripts": {
    "test": "npm run build && budo docs/code.js:index.js --dir docs --live",
    "build": "rollup docs/code.js -o docs/index.js",
    "pkg-roll": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/davidfig/intersects.git"
  },
  "author": "david figatner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidfig/intersects/issues"
  },
  "homepage": "https://github.com/davidfig/intersects#readme",
  "devDependencies": {
    "rollup": "^2.10.0",
    "rollup-plugin-terser": "^6.1.0",
    "budo": "^11.6.4",
    "calc-fontsize": "^1.1.0",
    "highlight.js": "^9.15.6",
    "pixi-ease": "^1.3.0",
    "pixi.js": "^5.0.2",
    "yy-random": "^1.8.0"
  }
}
